{
  "name": "cypress_web_api_with_pom",
  "version": "1.0.0",
  "description": "QA Code Quiz",
  "main": "webpack.config.js",
  "scripts": {
    "server": "node mockedAPI/index.js",
    "dev": "webpack-dev-server --open",
    "start": "npm-run-all --parallel dev server",
    "uitest": "npx cypress open",
    "start:test": "webpack-dev-server --config webpack.test.config.js",
    "test": "npx cypress run && npm run report",
    "report": "npx mochawesome-merge -f cypress/reports/mocha/*.json -o cypress/reports/mochawesome.json && npx marge cypress/reports/mochawesome.json --reportDir cypress/reports --reportTitle 'Cypress Test Report'"
  },
